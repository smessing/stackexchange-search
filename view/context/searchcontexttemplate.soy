{namespace view.context}

{template .searchContext}
  <div class='context' id='search-context'>
    {call searchBar /}
    {call searchInfo /}
  </div>
{/template}

{template .searchBar private="true"}
  <div id='search-bar'>
    <input id='search-bar-input'>
  </div>
{/template}

{template .searchInfo private="true"}
  <div id='search-info'>

  </div>
{/template}
